{
  "presets": [["env", { "modules": "commonjs" }], "flow", "next/babel"],
  "plugins": [
    "transform-decorators-legacy",
    [
      "import",
      {
        "libraryName": "antd"
      }
    ],
    ["styled-components", { "ssr": true, "displayName": true, "preprocess": false }]
  ],
  "env": {
    "development": {
      "plugins": [
        "flow-runtime",
        "transform-decorators-legacy",
        [
          "import",
          {
            "libraryName": "antd"
          }
        ],
        ["styled-components", { "ssr": true, "displayName": true, "preprocess": false }]
      ],
      "presets": [["env", { "modules": "commonjs" }], "flow", "next/babel"]
    },
    "test": {
      "plugins": ["flow-runtime"],
      "presets": [["env", { "modules": "commonjs" }], "next/babel"]
    }
  }
}
